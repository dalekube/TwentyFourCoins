server {
    server_name twentyfourcoins.com www.twentyfourcoins.com;
    access_log   /var/log/nginx/access.log;
    error_log    /var/log/nginx/error.log;

    location / {
    include proxy_params;
    proxy_pass http://unix:/tmp/twentyfourcoins.sock;
    proxy_headers_hash_bucket_size 128;
    }

    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/twentyfourcoins.com/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/twentyfourcoins.com/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot
}
server {
    if ($host = www.twentyfourcoins.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot

    if ($host = twentyfourcoins.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot

    listen 80;
    server_name twentyfourcoins.com www.twentyfourcoins.com;
    return 404; # managed by Certbot
}
server {
    server_name 18.191.29.151;
    listen 80;
    listen 443 ssl;
    return 404;
}
